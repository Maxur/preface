{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "createDefaultProgram": true
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "extends": [
    "prettier",
    "prettier/@typescript-eslint",
    "plugin:@typescript-eslint/recommended",
    "airbnb"
  ],
  "plugins": ["@typescript-eslint"],
  "env": {
    "commonjs": true,
    "browser": true,
    "node": true,
    "es6": true
  }
